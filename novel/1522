	<script type="text/javascript">
		var temp_lst = '';
		var total = volume_list.length;

		function appendList(){
			var style_hidden = 'isopen=1';
			for(i=0; i<total; i++){
				if(i > 0) style_hidden = 'style="display:none;" isopen=0';
				temp_lst += volume_list[i] + '<p class="xlist" '+ style_hidden +'>';
				temp_lst += chapter_list[i].reverse().join('') + '</p>';
			}
			$("#sort_div_p").html(temp_lst);
			temp_lst = '';
		}

		volume_list.reverse();
		chapter_list.reverse();
		appendList();

		function change_block(name){
			$("#"+name+"_click").attr("class","on").siblings().attr("class","");
			$("#"+name+"_block").css("display","block").siblings(".section4").css("display","none");
			//if(name == "comment" && !document.getElementById("tweets_area")) load_tweet();
		}

		function onSort(){
			if($("#sort_div").attr("sort") == "asc"){
				$("#sort_div").attr("sort","desc");
				volume_list.reverse();
				chapter_list.reverse();
				appendList();
				$("#sort_span").empty().append('倒序<em class="d"></em>');
			}else{
				$("#sort_div").attr("sort","asc");
				volume_list.reverse();
				chapter_list.reverse();
				appendList();
				$("#sort_span").empty().append('正序<em class="u"></em>');
			}
		}


		function showDetail(obj){
			var xlist = $(obj).parent(".chapname").next(".xlist");
			if(xlist.attr("isopen") == 1){
				xlist.slideUp("fast");
				xlist.attr("isopen",0);
			}else{
				xlist.slideDown("fast");
				xlist.attr("isopen",1);
			}
		}

		function load_tweet(){
			var href = location.href;
			// 获取参数列表
			var params = '';
			var delimiter = '?';
			for (key in resource_comment) {
				params += delimiter + key + '=' + resource_comment[key];
				delimiter = '&amp;';
			}
			params += "&amp;inset=true&amp;news_url="+escape(location.href);

			$("#comment_block").append('<span><iframe allowTransparency="true" id="tweets_area" src="http://t.178.com/resource/app_show'+params+'" width="100%"  scrolling="no" frameborder="0" height="0"></iframe></span>');
		}

		var shareBox={};
		shareBox.obj=document.getElementById('shareBox');
		shareBox.link=shareBox.obj.getElementsByTagName('a');
		shareBox.title=document.title;
		shareBox.url=location.href;
		for (var i=0;i<shareBox.link.length;i++){
			var tempHref=shareBox.link[i].getAttribute('href');
			tempHref = tempHref.replace(/topic/g,encodeURIComponent(shareBox.title));
			tempHref = tempHref.replace(/share_url/,encodeURIComponent(shareBox.url));
			shareBox.link[i].setAttribute('href',tempHref);
		}

		function toShare(){
			var panel_height = $(document).height() + 60;
			$("#mask_panel").height(panel_height).fadeIn("slow").click(function(){
				$("#pop_grade").slideUp("fast",function(){
					$("#mask_panel").fadeOut("slow");
				});
			});
			$("#pop_grade").slideDown("fast");
		}
